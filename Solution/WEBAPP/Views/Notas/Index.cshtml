@model IEnumerable<Core.Entity.Notas>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAlumnoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCursoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAlumnoNavigation.IdAlumno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCursoNavigation.Docente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdNota">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdNota">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdNota">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
